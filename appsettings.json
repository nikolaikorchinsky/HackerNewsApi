{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "HackerNewsApi": {
    "BaseUrl": "https://hacker-news.firebaseio.com/v0/",
    "BestStoriesEndpoint": "beststories.json",
    "ItemEndpoint": "item/{0}.json"
  },
  "CacheSettings": {
    "ExpirationMinutes": 10
  },
  "RateLimit": {
    "MaxRequests": 50,
    "IntervalSeconds": 1,
    "MaxQueue": 2,
    "SemaphoreLimit": 10
  }
}
